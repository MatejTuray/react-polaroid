{
  "description": "React component - Flippable Polaroid photo",
  "displayName": "Polaroid",
  "methods": [],
  "props": {
    "frontText": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Text on the front of the polaroid picture / card",
      "defaultValue": {
        "value": "\"Polaroid kitty - front\"",
        "computed": false
      }
    },
    "imgSrc": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Image uri",
      "defaultValue": {
        "value": "placecat",
        "computed": true
      }
    },
    "height": {
      "type": {
        "name": "number"
      },
      "required": false,
      "description": "Height",
      "defaultValue": {
        "value": "400",
        "computed": false
      }
    },
    "width": {
      "type": {
        "name": "number"
      },
      "required": false,
      "description": "Width",
      "defaultValue": {
        "value": "220",
        "computed": false
      }
    },
    "rotation": {
      "type": {
        "name": "number"
      },
      "required": false,
      "description": "Rotation of the component",
      "defaultValue": {
        "value": "0",
        "computed": false
      }
    },
    "flip": {
      "type": {
        "name": "bool"
      },
      "required": false,
      "description": "Control flipping between front and back",
      "defaultValue": {
        "value": "false",
        "computed": false
      }
    },
    "backText": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Text on the back of the polaroid picture / card",
      "defaultValue": {
        "value": "\"Polaroid kitty - back\"",
        "computed": false
      }
    },
    "disabled": {
      "type": {
        "name": "bool"
      },
      "required": false,
      "description": "Disable flipping",
      "defaultValue": {
        "value": "false",
        "computed": false
      }
    },
    "type": {
      "type": {
        "name": "enum",
        "value": [
          {
            "value": "\"side\"",
            "computed": false
          },
          {
            "value": "\"default\"",
            "computed": false
          }
        ]
      },
      "required": false,
      "description": "Flip animation type",
      "defaultValue": {
        "value": "\"default\"",
        "computed": false
      }
    },
    "cardColor": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Optional background color prop",
      "defaultValue": {
        "value": "\"white\"",
        "computed": false
      }
    }
  }
}
